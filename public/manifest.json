{
  "manifest_version": 2,
  "name": "Material App",
  "version": "2.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "chrome-icon.png",
    "128": "chrome-icon-lg.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  },
  "permissions": [
    "storage"
  ]
}
